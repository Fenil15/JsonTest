.{{sid}} * {
  box-sizing: border-box
 }

.{{sid}} .box {
  width: 250px;
  height: 150px;
  position: relative
 }

.{{sid}} .box .access-pro {
  width: 252px;
  height: 150px
 }

.{{sid}} .box .overlap {
  width: 250px;
  height: 150px;
  background: linear-gradient(180deg, #eaabf0 0%, #4623e9 100%);
  position: relative;
  border-radius: 20px
 }

.{{sid}} .box .upgrade-to-PRO-to {
  position: absolute;
  width: 183px;
  top: 26px;
  left: 34px;
  font-family: \"Poppins-SemiBold\", Helvetica;
  font-weight: 600;
  color: #ffffff;
  font-size: 14px;
  text-align: center;
  letter-spacing: 0.14px;
  line-height: normal
 }

.{{sid}} .box .CTA {
  all: unset;
  box-sizing: border-box;
  position: absolute;
  width: 205px;
  height: 40px;
  top: 88px;
  left: 24px
 }

.{{sid}} .box .overlap-group {
  width: 203px;
  height: 40px;
  background-color: #ffffff;
  box-shadow: 2px 4px 4px #4f29ea2b;
  position: relative;
  border-radius: 20px
 }

.{{sid}} .box .text-wrapper {
  position: absolute;
  top: 10px;
  left: 55px;
  font-family: \"Poppins-SemiBold\", Helvetica;
  font-weight: 600;
  color: #4925e9;
  font-size: 14px;
  text-align: center;
  letter-spacing: 0.14px;
  line-height: normal
 }
.{{sid}} .component-head-container {
  font-size: {{label_font_size}}{{label_font_size_unit}};
  font-weight: {{label_font_weight}};
  color: {{label_text_color}};
  text-align: {{label_text_alignment}}

}
.{{sid}} [actionable]{
  cursor:pointer

}